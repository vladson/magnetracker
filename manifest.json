{
    "manifest_version": 2,
    "name": "Magnetracker",
    "description": "This extension adds magnet links to rutracker.org site.",
    "version": "1.23",
    "author": "Vladislav Bogomolov <vladson4ik@gmail.com>",
    "icons": {
        "16": "resources/icon_16.png",
        "48": "resources/icon_48.png",
        "64": "resources/icon_64.png",
        "128": "resources/icon_128.png"
    },
    "web_accessible_resources": [
        "resources/*.png"
    ],
    "permissions": [
        "activeTab", "https://rutracker.org/*", "http://rutracker.org/*"
    ],
    "content_scripts": [
        {
            "js": ["libs/jquery-2.1.1.min.js", "libs/underscore-min.js", "build/page.js"],
            "css": ["resources/magnet.css"],
            "matches": ["http://rutracker.org/*", "http://rutracker.org/forum/viewtopic.php",
                "https://rutracker.org/*", "https://rutracker.org/forum/viewtopic.php"]
        }
    ]
}